<Window x:Class="WpfApp2.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Canvas>
        <Ellipse Fill="Indigo" Canvas.Left="30" Canvas.Top="30" Width="200" Height="200"></Ellipse>
        <Rectangle Fill="DarkRed" Width="60" Height="60" Canvas.Left="25" Canvas.Top="25" Panel.ZIndex="4"/>
        
        <Rectangle Fill="Yellow" Width="60" Height="60" Canvas.Left="50" Canvas.Top="50" Panel.ZIndex="5"/>
        
        <Rectangle Fill="White" Width="60" Height="60" Canvas.Left="75" Canvas.Top="75" Panel.ZIndex="3"/>
    </Canvas>
</Window>
